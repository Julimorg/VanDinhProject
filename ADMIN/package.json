{
  "name": "reception",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "dev:electron": "concurrently \"yarn dev\" \"wait-on http://localhost:5173 && yarn electron:start\"",
    "electron:start": "tsc -p electron && electron dist-electron/main.cjs",
    "build": "vite build && tsc -p electron",
    "build:all": "yarn build && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.zentryman.app",
    "productName": "Zentry Man",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "release"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "./public/zentryIcon.ico"
    },
    "asar": false
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@microsoft/signalr": "^8.0.7",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.6",
    "@tanstack/react-query": "^5.75.7",
    "@tanstack/react-query-devtools": "^5.75.7",
    "antd": "^5.25.2",
    "axios": "^1.9.0",
    "electron-is-dev": "^3.0.1",
    "flag-icon-css": "^4.1.7",
    "i18next": "^25.1.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "node-machine-id": "^1.1.12",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.1",
    "react-moment": "^1.1.3",
    "react-router-dom": "^7.6.0",
    "react-simple-keyboard": "^3.8.80",
    "react-toastify": "^11.0.5",
    "styled-components": "^6.1.18",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.0",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "9.0.11",
    "@storybook/addon-docs": "9.0.11",
    "@storybook/addon-onboarding": "9.0.11",
    "@storybook/addon-vitest": "9.0.11",
    "@storybook/react-vite": "9.0.11",
    "@tailwindcss/postcss": "^4.1.6",
    "@types/electron": "^1.6.12",
    "@types/node": "^22.15.17",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "9.0.11",
    "globals": "^16.0.0",
    "playwright": "^1.53.0",
    "postcss": "^8.5.3",
    "storybook": "9.0.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  }
}
